  "scripts": {
    "generate-build-version": "node generate-build-version",
    "prebuild": "yarn run generate-build-version",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "start:react": "react-scripts start",
    "build:react-prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:react": "GENERATE_SOURCEMAP=true react-scripts build",
    "start": "npm-run-all --parallel watch:css start:react",
    "build:prod": " npm-run-all build:css build:react-prod",
    "build": " npm-run-all build:css build:react",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint": "eslint src/**/*.css src/**/*.js src/**/*.jsx",
    "lint:fix": "eslint --fix \"src/**/*.+(js|jsx|json)\"",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "update:packages": "node wipe-dependencies.js && npm update --save-dev && npm update --save",
    "remove-unused-imports": "jscodeshift -t remove-unused-imports.js src --extensions=jsx,js --verbose=2 -d",
    "remove-console": "jscodeshift -t remove-consoles.js src --extensions=js,jsx --verbose=2"
  },
