{
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSpacing": true,
	"arrowParens": "avoid",
    "semi": false,
    "proseWrap": "never",
    "insertPragma": true,
    "trailingComma": "es5",
    "bracketSameLine": false,
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxBracketSameLine": false,
	"printWidth": 140
}
