{
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "insertPragma": true,
    "trailingComma": "es5",
    "bracketSameLine": true,
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxBracketSameLine": true
}