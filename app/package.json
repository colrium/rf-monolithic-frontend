    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-router-sitemap": "^1.2.0",
    "redux-devtools": "^3.5.0",
    "tailwindcss": "^1.1.3"
  },
  "resolutions": {
    "@material-ui/styles": "4.1.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "globals": {
      "google": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "rules": {
      "no-console": 0,
      "no-extend-native": 0,
      "react/display-name": 0,
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "array-bracket-newline": 0,
      "function-call-argument-newline": 0,
      "function-paren-newline": 0,
      "max-params": 0,
      "no-unreachable": 0,
      "react/no-direct-mutation-state": 0,
      "react-hooks/exhaustive-deps": 0,
      "react/prop-types": 0,
      "no-unused-vars": "off",
      "no-dupe-keys": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "insertPragma": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
